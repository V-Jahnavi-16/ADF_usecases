			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "2016_Medals",
								"type": "DatasetReference"
							},
							"name": "Usecase2"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "gold",
								"type": "DatasetReference"
							},
							"name": "gold"
						},
